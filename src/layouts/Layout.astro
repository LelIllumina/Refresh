---
import Head from "@components/Head/Head.astro";
import "@styles/global.css";

const { title, description, image } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Head title={title} description={description} image={image} />
  <body>
    <body>
      <header></header>
      <main>
        <slot />
      </main>
      <footer></footer>
    </body>
  </body>
</html>
